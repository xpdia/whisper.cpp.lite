{
  "dependencies": {
    "child_process": "^1.0.2",
    "express": "^4.21.2",
    "multer": "^1.4.5-lts.1"
  },
  "scripts": {
    "start": "node server.js"
  },
  "vercel": {
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ]
  }
}